<%= render partial: "layouts/errors", locals: {object: @user} %>

<%= form_for @user do |f| %>
  
    <label>Username:</label><br>
  <%= f.text_field :username %></br>

  <label>Email:</label><br>
  <%= f.email_field:email %></br>

  <label>Password:</label><br>
  <%= f.password_field :password %></br>

  <%= f.submit %>
<% end %>